<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <queries>
        <intent>
            <action android:name="android.intent.action.VIEW" />
            <data android:scheme="geo" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_eatarai"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_eatarai_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EATArai"
        tools:targetApi="31">
        <activity
            android:name=".StartActivity"
            android:exported="true"
            android:theme="@style/Theme.EATArai">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChooseFoodDessert"/>
        <activity android:name=".ChooseOption"/>
        <activity android:name=".ChooseCuisineActivity"/>
        <activity android:name=".CuisineActivity" />
        <activity android:name=".MenuActivity"/>
        <activity android:name=".MenuSummaryActivity"/>
        <activity android:name=".HistoryActivity" />
    </application>

</manifest>